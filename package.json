{
  "name": "express-scaffold",
  "private": true,
  "version": "1.0.0",
  "description": "the api server for express-scaffold",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanacial/express-scaffold.git"
  },
  "keywords": [
    "relief",
    "relief os"
  ],
  "author": "Philip S Acierto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alanacial/express-scaffold/issues"
  },
  "homepage": "https://github.com/alanacial/express-scaffold#readme",
  "dependencies": {
    "body-parser": "^1.14.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "mongodb": "^2.2.11",
    "morgan": "^1.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
